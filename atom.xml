---
layout: nil
---
<?xml version='1.0' encoding='utf-8'?>
<feed xmlns='http://www.w3.org/2005/Atom'>
  <title>Tom MacWright</title>
  <link href='http://tristen.ca/atom.xml' rel='self'/>
  <link href='http://tristen.ca/'/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://tristen.ca/</id>
  <author>
    <name>Tristen</name>
    <email>tristen.brown@gmail.com</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href='http://tristen.ca{{ post.url }}'/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://tristen.ca{{ post.id }}</id>
    <content type='html'>{{ post.content | xml_escape }}</content>
    <author>
      <name>Tristen Brown</name>
      <uri>http://tristen.ca/about/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>
